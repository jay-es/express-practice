{
  "name": "express-practice",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env DEBUG=express-practice:* nodemon ./bin/www",
    "eslint": "eslint --ext .js .",
    "mocha": "cross-env NODE_ENV=test mocha \"./{,!(node_modules)/**}/_tests/*.spec.js\"",
    "mocha:watch": "cross-env NODE_ENV=test npm run mocha -- --reporter dot --watch",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^5.5.7",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "sinon": "^7.3.2"
  }
}
